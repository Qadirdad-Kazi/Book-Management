{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
        }
    ],
    "env": {
        "MONGODB_URI": "${MONGODB_URI}",
        "JWT_SECRET": "${JWT_SECRET}",
        "FRONTEND_URL": "https://kazibookmanagement.netlify.app"
    }
}
